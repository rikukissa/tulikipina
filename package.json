{
  "name": "gulp-template",
  "version": "0.0.0",
  "description": "Project template for gulp.js",
  "main": "gulpfile.js",
  "scripts": {
    "prepublish": "bower install --silent",
    "start": "gulp",
    "build": "NODE_ENV=production gulp build"
  },
  "keywords": [
    "gulp",
    "template"
  ],
  "author": "",
  "dependencies": {
    "instafeed.js": "1.3.2",
    "knockout": "3.3.0",
    "leaflet": "0.7.5",
    "lodash": "3.10.1",
    "marked": "0.3.2",
    "timeago": "0.2.0"
  },
  "devDependencies": {
    "bluebird": "2.10.2",
    "bower": "1.3.12",
    "browserify": "11.1.0",
    "browserify-plain-jade": "0.2.2",
    "browserify-shim": "3.8.10",
    "coffee-script": "1.10.0",
    "coffeeify": "1.1.0",
    "contentful": "1.2.0",
    "event-stream": "3.1.5",
    "express": "4.17.1",
    "gulp": "3.9.0",
    "gulp-autoprefixer": "0.0.6",
    "gulp-combine-media-queries": "0.0.1",
    "gulp-concat": "2.2.0",
    "gulp-cson": "0.0.1",
    "gulp-imagemin": "0.5.0",
    "gulp-jade": "1.1.0",
    "gulp-less": "1.2.3",
    "gulp-livereload": "0.2.0",
    "gulp-minify-css": "0.3.3",
    "gulp-plumber": "0.5.6",
    "gulp-rename": "0.2.1",
    "gulp-replace": "0.2.0",
    "gulp-rev": "0.3.2",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "2.0.7",
    "gulp-uglify": "0.1.0",
    "gulp-util": "2.2.9",
    "imagemin-gifsicle": "5.1.0",
    "imagemin-optipng": "5.1.1",
    "imagemin-pngquant": "6.0.0",
    "rimraf": "2.2.6",
    "stylus": "0.54.0",
    "tiny-lr": "0.0.5",
    "vinyl-source-stream": "0.1.1",
    "watchify": "3.4.0"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "scrollTo": "./bower_components/jquery.scrollTo/jquery.scrollTo.js",
    "modernizr": "./bower_components/modernizr/modernizr.js",
    "routie": "./bower_components/routie/dist/routie.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "modernizr": "Modernizr",
    "routie": "routie"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim",
      "browserify-plain-jade"
    ]
  }
}
